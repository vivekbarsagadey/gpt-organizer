{
    "manifest_version": 3,
    "name": "GPT Organizer",
    "version": "1.0",
    "description": "Organize and manage the ChatGPT homepage menu for a better experience.",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "host_permissions": [
      "https://chat.openai.com/*"
    ]
  }
  